# Example Environment Variables
# Copy this file to .dev.vars for development or configure in Cloudflare Workers for production

# Database Configuration
DATABASE_URL="file:./dev.db"
D1_DATABASE_URL="your-d1-database-url"

# OpenAI Configuration
OPENAI_API_KEY="your-openai-api-key"
OPENAI_MODEL="gpt-3.5-turbo"

# Authentication (Stack Auth)
STACK_AUTH_PROJECT_ID="your-stack-auth-project-id"
STACK_AUTH_SECRET_KEY="your-stack-auth-secret"

# Application Configuration
NEXT_PUBLIC_APP_URL="http://localhost:3003"
NODE_ENV="development"

# Optional Configuration
RATE_LIMIT_MAX="100"
RATE_LIMIT_WINDOW="3600"
ANALYTICS_ENABLED="true"

# Development Only
DEBUG_MODE="true"
LOG_LEVEL="info"
